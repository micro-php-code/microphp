{
    "name": "microphp/microphp",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "liutao",
            "email": "gg.cn.lt@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "microphp/framework": "dev-master",
        "microphp/workerman": "dev-master",
        "microphp/roadrunner": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.22.0",
        "phpunit/phpunit": "^10.2"
    },
    "scripts": {
        "cs": [
            "./vendor/bin/php-cs-fixer fix --verbose"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "microphp": {
            "type": "path",
            "url": "../*"
        }
    }
}
